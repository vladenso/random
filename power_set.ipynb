    "def power_set(arr, per=[], i=0):\n",
    "    if i == len(arr):\n",
    "        print(per)\n",
    "        return\n",
    "    power_set(arr, per+[arr[i]], i+1)\n",
    "    power_set(arr, per, i+1)"
